@model IEnumerable<Item>

@{

}
<a asp-controller="Panel" asp-action="Edit">
    Add New Item
</a>

<div>
    <ul>
        @foreach (var item in Model)
       {
           <li>
               <a asp-controller="Home" asp-action="Item" asp-route-id="@item.Id">
                   @item.Title
               </a>
               -
               <a asp-controller="Panel" asp-action="Edit" asp-route-id="@item.Id">
                   Edit
               </a>
               -
               <a asp-controller="Panel" asp-action="Remove" asp-route-id="@item.Id">
                   Remove
               </a>
           </li>
       }
    </ul>
</div>